    location /assets/ {
        root /data/public/{{ endpointData['entry-point'] }};
        access_log  off;
        expires     30d;
        add_header  Pragma public;
        add_header  Cache-Control "public, must-revalidate, proxy-revalidate";
        try_files   $uri =404;
    }

    location / {
        root /data/public/{{ endpointData['entry-point'] }};
        access_log  off;
        expires     30d;
        add_header  Pragma public;
        add_header  Cache-Control "public, must-revalidate, proxy-revalidate";
        try_files   $uri @proxy;
    }
