        fastcgi_param SPRYKER_SEARCH_NAMESPACE "{{ regionServices['search']['namespace'] | default(endpointData['services']['search']['namespace'])  }}";
        fastcgi_param SPRYKER_KEY_VALUE_STORE_NAMESPACE "{{ regionServices['key_value_store']['namespace'] | default(endpointData['services']['key_value_store']['namespace'])  }}";
        fastcgi_param SPRYKER_BROKER_NAMESPACE "{{ regionServices['broker']['namespace'] | default(endpointData['services']['broker']['namespace'])  }}";
