  {{ applicationName }}:
    image: ${SPRYKER_DOCKER_PREFIX}_app:${SPRYKER_DOCKER_TAG}-{{ applicationData['application']}}
    depends_on:
      - database
      - broker
      - key_value_store
      - session
      - search
    networks:
      - services
      - private
    environment:
{% for envName, envValue in _envs %}
      {{ envName }}: {{ "${%s}" | format(envName) }}
{% endfor %}
    env_file:
      - ${DEPLOYMENT_PATH}/env/{{ applicationName }}.env
    <<: *app-volumes
