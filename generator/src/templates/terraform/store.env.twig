export APPLICATION_STORE={{ storeName | env_var }}
export SPRYKER_SEARCH_NAMESPACE={{ services['search']['namespace'] | env_var }}
export SPRYKER_KEY_VALUE_STORE_NAMESPACE={{ services['key_value_store']['namespace'] | env_var }}
export SPRYKER_BROKER_NAMESPACE={{ services['broker']['namespace'] | env_var }}
export SPRYKER_SESSION_BE_NAMESPACE={{ services['session']['namespace'] | env_var }}
export SPRYKER_FE_HOST={{ endpointMap[storeName]['yves'] | default('')  | split(':') | first | env_var }}
export SPRYKER_API_HOST={{ endpointMap[storeName]['glue'] | default('') | split(':') | first | env_var }}
export SPRYKER_BE_HOST={{ endpointMap[storeName]['zed'] | default('')  | split(':') | first | env_var }}
export SPRYKER_SSL_ENABLE="{{ project['docker']['ssl']['enabled'] ? 1 : 0}}"
export SPRYKER_ACTIVE_STORES={{ project['regions'][regionName]['stores'] | keys | join(',') | env_var }}
export SPRYKER_STORE_SPECIFIC={{ project['_storeSpecific'] | env_var }}
