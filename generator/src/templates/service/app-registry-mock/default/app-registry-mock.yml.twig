  {{ serviceName }}:
    image: spryker/app-registry-mock:latest-{{ get_sys_arch() }}
    networks:
      - private
    labels:
      'spryker.app.name': app-registry-mock
      'spryker.app.type': services
      'spryker.project': ${SPRYKER_DOCKER_PREFIX}:${SPRYKER_DOCKER_TAG}
    volumes:
      - {{ serviceName }}-{{ serviceData['engine'] }}-data:/var/app-registry-mock:rw
