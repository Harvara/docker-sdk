  {{ serviceName }}:
    image: rediscommander/redis-commander:latest
    networks:
      - services
      - private
    restart: always
    environment:
      REDIS_HOSTS: "{{ serviceData['data']['hosts'] | default([]) | join(',') }}"
    depends_on:
{% for server in serviceData['data']['servers'] %}
      - {{ server }}
{% endfor %}
