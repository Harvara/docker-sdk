  {{ serviceData.project_name ~ '_' ~ serviceName }}:
    image: spryker/redis-commander:0.8.0
    container_name: {{ serviceData.project_name ~ '_' ~ serviceName }}
    profiles:
      - {{ serviceData.project_name }}
    networks:
      - {{ _internal_project_name }}_private
    labels:
      'spryker.app.name': {{ serviceData.project_name ~ '_' ~ serviceName }}
      'spryker.app.type': hidden
      'spryker.project': ${SPRYKER_DOCKER_PREFIX}:${SPRYKER_DOCKER_TAG}
    environment:
{#          todo: fix #}
      REDIS_HOSTS: "{{ serviceData.storageData.hosts | join(',') }}"
    depends_on:
{% for service in serviceData.storageData.services %}
      - {{ service }}
{% endfor %}
